{
"name": "pollen-paw-backend",
"version": "1.0.0",
"description": "Pollen Paw Backend API",
"main": "dist/index.js",
"scripts": {
    "build": "tsc",
    "dev": "serverless offline",
    "deploy": "serverless deploy",
    "deploy:prod": "serverless deploy --stage prod",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "node -r dotenv/config database/run-migration.js",
    "db:seed": "node -r dotenv/config database/run-seed.js"
  },
  
  "keywords": ["pet", "allergy", "tracking"],
  "author": "Gina Song",
  "license": "MIT",

  
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.972.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.17.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.9",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "prettier": "^3.8.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
