{
  "name": "pollen-paw-backend",
  "version": "1.0.0",
  "description": "Pollen Paw Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "serverless offline",
    "build": "tsc",
    "deploy": "serverless deploy --aws-profile pollen-paw",
    "deploy:prod": "serverless deploy --stage prod --aws-profile pollen-paw",
    "logs": "serverless logs -f getPollenData --tail --aws-profile pollen-paw",
    "remove": "serverless remove --aws-profile pollen-paw",
    "test": "jest",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest src/utils",
    "test:integration": "jest src/handlers",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:migrate": "node -r dotenv/config database/run-migration.js",
    "db:seed": "node -r dotenv/config database/run-seed.js"
  },
  "keywords": [
    "pet",
    "allergy",
    "tracking"
  ],
  "author": "Gina Song",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "axios": "^1.13.3",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.17.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "dotenv-cli": "^11.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.8.0",
    "serverless": "^3.40.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.57.0",
    "serverless-offline": "^13.9.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.9.3"
  }
}
